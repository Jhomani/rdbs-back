  "paths": {
    "/users/login": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegionPrice"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "$ref": "#/components/schemas/RegionPrice"
                }
              }
            }
          }
        }
      }
    },
    "/users/register": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "$ref": "#/components/schemas/User_Model"
                }
              }
            }
          }
        }
      }
    },
    "/users/me": {
      "get": {
        "tags": [
          "User"
        ],
        "security": [
          {
            "Bearer": [
              "token"
            ]
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json; charset=utf-8": {
                "schema": {
                  "$ref": "#/components/schemas/RegionPrice"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "$ref": "/schemas.json"
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "For this sample, you can use the api key `special-key` to access some endpoints, you get Api Key on User profile seccion.\n",
        "in": "header",
        "name": "Authorization"
      }
    }
  }
